<?xml version="1.0"?>
<flowgorithm fileversion="3.0">
    <attributes>
        <attribute name="name" value=""/>
        <attribute name="authors" value="Leo"/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2023-12-22 10:07:11 PM"/>
        <attribute name="created" value="TGVvO0RFU0tUT1AtSUZQTTJJMjsyMDIzLTEyLTA0OzA0OjU1OjU2IFBNOzI2Mjc="/>
        <attribute name="edited" value="TGVvO0RFU0tUT1AtSUZQTTJJMjsyMDIzLTEyLTIyOzEwOjA3OjExIFBNOzcyOzI3NzY="/>
    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="board init"/>
            <declare name="boardHeight, boardWidth" type="Integer" array="False" size=""/>
            <assign variable="boardHeight" expression="12"/>
            <assign variable="boardWidth" expression="24"/>
            <comment text="border"/>
            <declare name="border" type="String" array="False" size=""/>
            <assign variable="border" expression="makeBorder(boardWidth)"/>
            <comment text="snake"/>
            <declare name="snakeCords" type="Integer" array="True" size="2"/>
            <assign variable="snakeCords[0]" expression="8"/>
            <assign variable="snakeCords[1]" expression="8"/>
            <comment text="2d array"/>
            <declare name="matrix, item" type="Integer" array="True" size="2"/>
            <assign variable="matrix[0]" expression="item[2]"/>
            <call expression="render(boardHeight,boardWidth,border,snakeCords)"/>
        </body>
    </function>
    <function name="makeBorder" type="String" variable="border">
        <parameters>
            <parameter name="boardWidth" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="border" type="String" array="False" size=""/>
            <declare name="i" type="Integer" array="False" size=""/>
            <assign variable="border" expression="&quot;+&quot;"/>
            <for variable="i" start="0" end="boardWidth" direction="inc" step="1">
                <assign variable="border" expression="border &amp; &quot;-&quot;"/>
            </for>
            <assign variable="border" expression="border &amp; &quot;+&quot;"/>
        </body>
    </function>
    <function name="render" type="None" variable="">
        <parameters>
            <parameter name="boardHeight" type="Integer" array="False"/>
            <parameter name="boardWidth" type="Integer" array="False"/>
            <parameter name="border" type="String" array="False"/>
            <parameter name="snakeCords" type="Integer" array="True"/>
        </parameters>
        <body>
            <declare name="n" type="Integer" array="False" size=""/>
            <declare name="emptyChar, snakeChar" type="String" array="False" size=""/>
            <assign variable="emptyChar" expression="&quot; &quot;"/>
            <declare name="displayArr" type="String" array="True" size="boardWidth"/>
            <declare name="displayRow" type="String" array="False" size=""/>
            <assign variable="displayRow" expression="&quot;|&quot;"/>
            <for variable="n" start="0" end="boardWidth" direction="inc" step="1"/>
            <comment text=""/>
            <for variable="n" start="0" end="snakeCords[0] - 1" direction="inc" step="1">
                <assign variable="displayRow" expression="displayRow &amp; emptyChar"/>
            </for>
            <assign variable="displayRow" expression="displayRow &amp; &quot;x&quot;"/>
            <for variable="n" start="snakeCords[0] + 1" end="boardWidth" direction="inc" step="1">
                <assign variable="displayRow" expression="displayRow &amp; emptyChar"/>
            </for>
            <assign variable="displayRow" expression="displayRow &amp; &quot;|&quot;"/>
            <for variable="n" start="0" end="boardHeight - 1" direction="inc" step="1">
                <assign variable="displayArr[n]" expression="displayRow"/>
            </for>
            <open expression="&quot;display.md&quot;" mode="write"/>
            <write expression="border"/>
            <for variable="n" start="0" end="boardHeight - 1" direction="inc" step="1">
                <write expression="displayArr[n]"/>
            </for>
            <write expression="border"/>
            <close/>
        </body>
    </function>
</flowgorithm>
