    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="randPosition, snakeHeadPosition, applePosition" type="Integer" array="True" size="2"/>
            <declare name="randHeight" type="Integer" array="False" size=""/>
            <assign variable="randHeight" expression="11"/>
            <declare name="randWidth" type="Integer" array="False" size=""/>
            <assign variable="randWidth" expression="22"/>
            <declare name="Score" type="Integer" array="False" size=""/>
            <assign variable="Score" expression="0"/>
            <declare name="steps" type="Integer" array="False" size=""/>
            <assign variable="steps" expression="0"/>
            <declare name="AppleHasSpawned" type="Boolean" array="False" size=""/>
            <assign variable="AppleHasSpawned" expression="false"/>
            <call expression="randomCordArray(applePosition,randHeight,randWidth)"/>
            <assign variable="snakeHeadPosition[0]" expression="0"/>
            <assign variable="snakeHeadPosition[1]" expression="0"/>
            <while expression="Score &lt; 10">
                <if expression="AppleSpawn(steps,AppleHasSpawned)">
                    <then>
                        <do expression="snakeHeadPosition[0] == applePosition[0] &amp;&amp; snakeHeadPosition[1] == applePosition[1]">
                            <call expression="randomCordArray(applePosition,randHeight,randWidth)"/>
                        </do>
                        <assign variable="AppleHasSpawned" expression="true"/>
                    </then>
                    <else/>
                </if>
                <if expression="applePosition[0] == 0 &amp;&amp; applePosition[1] == 0">
                    <then/>
                    <else>
                        <output expression="&quot;Apple is at: &quot; &amp; applePosition[0] &amp; &quot; &quot; &amp;applePosition[1]" newline="True"/>
                    </else>
                </if>
                <output expression="&quot;Enter snake x position &quot;" newline="True"/>
                <input variable="snakeHeadPosition[0]"/>
                <output expression="&quot;Enter snake y position &quot;" newline="True"/>
                <input variable="snakeHeadPosition[1]"/>
                <assign variable="steps" expression="steps+1"/>
                <if expression="isSamePosition(applePosition,snakeHeadPosition)">
                    <then>
                        <assign variable="Score" expression="Score+1"/>
                        <assign variable="AppleHasSpawned" expression="false"/>
                        <comment text="reset of apple"/>
                        <assign variable="applePosition[0]" expression="0"/>
                        <assign variable="applePosition[1]" expression="0"/>
                    </then>
                    <else/>
                </if>
                <output expression="&quot;Score &quot; &amp; Score" newline="True"/>
            </while>
        </body>
    </function>
    <function name="AppleSpawn" type="Boolean" variable="AppleHasSpawned">
        <parameters>
            <parameter name="steps" type="Integer" array="False"/>
            <parameter name="AppleHasSpawned" type="Boolean" array="False"/>
        </parameters>
        <body>
            <if expression="steps % 8 == 0 and AppleHasSpawned == false">
                <then>
                    <assign variable="AppleHasSpawned" expression="true"/>
                </then>
                <else>
                    <assign variable="AppleHasSpawned" expression="false"/>
                </else>
            </if>
        </body>
    </function>
    <function name="isSamePosition" type="Boolean" variable="positionIsEqual">
        <parameters>
            <parameter name="snakePosition" type="Integer" array="True"/>
            <parameter name="applePosition" type="Integer" array="True"/>
        </parameters>
        <body>
            <declare name="positionIsEqual" type="Boolean" array="False" size=""/>
            <if expression="snakePosition[0] == applePosition[0] &amp;&amp; snakePosition[1] == applePosition[1]">
                <then>
                    <assign variable="positionIsEqual" expression="true"/>
                </then>
                <else>
                    <assign variable="positionIsEqual" expression="false"/>
                </else>
            </if>
        </body>
    </function>
    <function name="randomCordArray" type="None" variable="">
        <parameters>
            <parameter name="randomPosition" type="Integer" array="True"/>
            <parameter name="boardHeight" type="Integer" array="False"/>
            <parameter name="boardWidth" type="Integer" array="False"/>
        </parameters>
        <body>
            <assign variable="randomPosition[0]" expression="random(boardHeight)"/>
            <assign variable="randomPosition[1]" expression="random(boardWidth)"/>
        </body>
    </function>
</flowgorithm>
