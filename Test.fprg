    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <open expression="&quot;display.md&quot;" mode="write"/>
            <declare name="displayArr" type="String" array="True" size="8"/>
            <declare name="pixel, charColumn" type="String" array="False" size=""/>
            <declare name="n, i" type="Integer" array="False" size=""/>
            <assign variable="pixel" expression="&quot;*&quot;"/>
            <assign variable="charColumn" expression="&quot;|&quot;"/>
            <for variable="i" start="0" end="8" direction="inc" step="1">
                <assign variable="charColumn" expression="charColumn &amp; pixel"/>
            </for>
            <for variable="n" start="0" end="7" direction="inc" step="1">
                <assign variable="displayArr[n]" expression="charColumn"/>
            </for>
            <for variable="n" start="0" end="7" direction="inc" step="1">
                <write expression="displayArr[n]"/>
            </for>
            <close/>
        </body>
    </function>
