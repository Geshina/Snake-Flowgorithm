<?xml version="1.0"?>
<flowgorithm fileversion="3.0">
    <attributes>
        <attribute name="name" value=""/>
        <attribute name="authors" value="Leo"/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2023-12-22 02:26:53 PM"/>
        <attribute name="created" value="TGVvO0RFU0tUT1AtSUZQTTJJMjsyMDIzLTEyLTA0OzA0OjU1OjU2IFBNOzI2Mjc="/>
        <attribute name="edited" value="TGVvO0RFU0tUT1AtSUZQTTJJMjsyMDIzLTEyLTIyOzAyOjI2OjUzIFBNOzQ3OzI3ODY="/>
    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="boardHeight, boardWidth" type="Integer" array="False" size=""/>
            <assign variable="boardHeight" expression="12"/>
            <assign variable="boardWidth" expression="24"/>
            <call expression="render(boardHeight,boardWidth)"/>
        </body>
    </function>
    <function name="borderHorizontal" type="String" variable="borderHorizontal">
        <parameters>
            <parameter name="boardWidth" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="borderHorizontal" type="String" array="False" size=""/>
            <assign variable="borderHorizontal" expression="&quot;+&quot;"/>
            <declare name="i" type="Integer" array="False" size=""/>
            <for variable="i" start="0" end="boardWidth" direction="inc" step="1">
                <assign variable="borderHorizontal" expression="boardWidth &amp; &quot;-&quot;"/>
            </for>
        </body>
    </function>
    <function name="borderVertical" type="String" variable="borderVertical">
        <parameters>
            <parameter name="boardHeight" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="borderVertical" type="String" array="False" size=""/>
            <assign variable="borderVertical" expression="&quot;+&quot;"/>
            <declare name="i" type="Integer" array="False" size=""/>
            <for variable="i" start="0" end="boardHeight" direction="inc" step="1">
                <assign variable="borderVertical" expression="borderVertical &amp; &quot;-&quot;"/>
            </for>
            <assign variable="borderVertical" expression="borderVertical &amp; &quot;+&quot;"/>
        </body>
    </function>
    <function name="render" type="None" variable="">
        <parameters>
            <parameter name="boardHeight" type="Integer" array="False"/>
            <parameter name="boardWidth" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="n" type="Integer" array="False" size=""/>
            <declare name="emptyChar, snakeChar" type="String" array="False" size=""/>
            <assign variable="emptyChar" expression="&quot; &quot;"/>
            <declare name="displayArr" type="String" array="True" size="boardWidth"/>
            <declare name="displayRow" type="String" array="False" size=""/>
            <assign variable="displayRow" expression="emptyChar"/>
            <for variable="n" start="0" end="boardWidth" direction="inc" step="1">
                <assign variable="displayRow" expression="displayRow &amp; emptyChar"/>
            </for>
            <for variable="n" start="0" end="boardHeight - 1" direction="inc" step="1">
                <assign variable="displayArr[n]" expression="displayRow"/>
            </for>
            <open expression="&quot;display.md&quot;" mode="write"/>
            <for variable="n" start="0" end="boardHeight - 1" direction="inc" step="1">
                <write expression="displayArr[n]"/>
            </for>
            <declare name="borderVertical, borderHorizontal" type="String" array="False" size=""/>
            <assign variable="borderVertical" expression="borderVertical(boardHeight)"/>
            <assign variable="borderHorizontal" expression="borderHorizontal(boardWidth)"/>
            <write expression="borderVertical"/>
            <write expression="borderHorizontal"/>
            <close/>
        </body>
    </function>
</flowgorithm>
