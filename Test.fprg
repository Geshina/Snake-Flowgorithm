<flowgorithm fileversion="3.0">
    <attributes>
        <attribute name="name" value=""/>
        <attribute name="authors" value="Leo"/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2023-12-23 07:37:23 PM"/>
        <attribute name="created" value="TGVvO0RFU0tUT1AtSUZQTTJJMjsyMDIzLTEyLTA0OzA0OjU1OjU2IFBNOzI2Mjc="/>
        <attribute name="edited" value="TGVvO0RFU0tUT1AtSUZQTTJJMjsyMDIzLTEyLTIzOzA3OjM3OjIzIFBNOzg4OzI3OTY="/>
    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="boardDimensions" type="Integer" array="True" size="2"/>
            <assign variable="boardDimensions[0]" expression="12"/>
            <assign variable="boardDimensions[1]" expression="24"/>
            <comment text=""/>
            <declare name="gameBoard" type="String" array="True" size="288"/>
            <declare name="i" type="Integer" array="False" size=""/>
            <for variable="i" start="0" end="288 - 1" direction="inc" step="1">
                <assign variable="gameBoard[i]" expression="&quot;*&quot;"/>
            </for>
            <call expression="render(boardDimensions,gameBoard)"/>
        </body>
    </function>
    <function name="render" type="None" variable="">
        <parameters>
            <parameter name="boardDimensions" type="Integer" array="True"/>
            <parameter name="gameBoard" type="String" array="True"/>
        </parameters>
        <body>
            <declare name="j, k, l" type="Integer" array="False" size=""/>
            <assign variable="j" expression="boardDimensions[0]"/>
            <assign variable="k" expression="boardDimensions[1]"/>
            <assign variable="l" expression="j*k"/>
            <comment text=""/>
            <declare name="displayTemp" type="String" array="False" size=""/>
            <open expression="&quot;Display.md&quot;" mode="write"/>
            <declare name="i, n" type="Integer" array="False" size=""/>
            <for variable="i" start="0" end="j" direction="inc" step="1">
                <assign variable="displayTemp" expression="&quot;|&quot;"/>
                <for variable="n" start="0" end="k" direction="inc" step="1">
                    <assign variable="displayTemp" expression="displayTemp &amp; gameBoard[n]"/>
                </for>
                <assign variable="displayTemp" expression="displayTemp &amp; &quot;|&quot;"/>
                <write expression="displayTemp"/>
            </for>
            <close/>
        </body>
    </function>
</flowgorithm>
